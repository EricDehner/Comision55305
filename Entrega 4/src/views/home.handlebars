<article>
    <h1 class="card_title">Productos</h1>
    <div class="card_container">
        {{#each listaProductos}}
        <div class="card">
            <div class="card_img">
                <img src={{thumbnail}} alt={{description}} />
            </div>
            <div class="card_content">
                <h2 class="card_content-title">{{title}}</h2>
                <h3 class="card_content-description">{{description}}</h3>
                <p class="card_content-price">${{price}}</p>
                <a class="card_content-btn" href="#">Comprar</a>
            </div>
        </div>
        {{/each}}
    </div>
</article>